{
  "expo": {
    "name": "SL_TRAFFIC_GUARD_PRO",
    "slug": "SL_TRAFFIC_GUARD_PRO",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "SL_TRAFFIC_GUARD_PRO",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to send SOS messages with your current location.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to send SOS messages with your current location."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "SEND_SMS",
        "READ_SMS",
        "RECEIVE_SMS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
    
  }
}
